<h1>New Place</h1>

<%= form_for([@trip, @trip.places.build]) do |f| %>
  <p>
    <%= f.label :place %><br>
    <%= f.text_field :place %>
  </p>

  <p>
    <%= f.label :start_date %><br>
    <%= f.text_field :start_date %>
  </p>

	<p>
    <%= f.label :end_date %><br>
    <%= f.text_field :end_date %>
  </p>

	<p>
    <%= f.label :latitude %><br>
    <%= f.text_field :latitude %>
  </p>

	<p>
    <%= f.label :longitude %><br>
    <%= f.text_field :longitude %>
  </p>

  <p>
    <%= f.label :submit %>
    <%= f.submit %>
  </p>
<% end %>